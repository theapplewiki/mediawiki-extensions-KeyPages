{
	"name": "KeyPages",
	"author": [
		"[[User:kirb|Adam Demasi]]"
	],
	"url": "https://theapplewiki.com/",
	"descriptionmsg": "keypages-desc",
	"type": "other",
	"license-name": "GPL-2.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"MessagesDirs": {
		"KeyPages": [
			"/i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"KeyPagesAlias": "KeyPages.i18n.alias.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\KeyPages\\": "includes/"
	},
	"SpecialPages": {
		"KeyPages": {
			"class": "MediaWiki\\Extension\\KeyPages\\SpecialKeyPages",
			"services": [
				"DBLoadBalancerFactory",
				"LinkBatchFactory",
				"LinksMigration"
			]
		}
	},
	"HookHandlers": {
		"Hooks": {
			"class": "MediaWiki\\Extension\\KeyPages\\Hooks"
		}
	},
	"Hooks": {
		"wgQueryPages": "Hooks"
	},
	"manifest_version": 2
}
