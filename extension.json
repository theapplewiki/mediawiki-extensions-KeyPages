{
	"name": "KeyPages",
	"author": [
		"[[User:kirb|Adam Demasi]]"
	],
	"url": "https://theapplewiki.com/",
	"descriptionmsg": "keypages-desc",
	"type": "other",
	"license-name": "GPL-2.0-or-later",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"MessagesDirs": {
		"KeyPages": [
			"/i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"WantedKeysAlias": "WantedKeys.i18n.alias.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\KeyPages\\": "includes/"
	},
	"namespaces": [
		{
			"id": 2304,
			"constant": "NS_KEYS",
			"name": "Keys",
			"subpages": false,
			"content": false
		},
		{
			"id": 2305,
			"constant": "NS_KEYS_TALK",
			"name": "Keys_talk",
			"subpages": false,
			"content": false
		}
	],
	"SpecialPages": {
		"WantedKeys": {
			"class": "MediaWiki\\Extension\\KeyPages\\SpecialWantedKeys",
			"services": [
				"DBLoadBalancerFactory",
				"LinkBatchFactory",
				"LinksMigration"
			]
		}
	},
	"ContentHandlers": {
		"keys":  {
			"class": "MediaWiki\\Extension\\KeyPages\\KeysContentHandler",
			"services": [
				"ParserFactory",
				"ParsoidParserFactory"
			]
		}
	},
	"HookHandlers": {
		"Hooks": {
			"class": "MediaWiki\\Extension\\KeyPages\\Hooks"
		}
	},
	"Hooks": {
		"wgQueryPages": "Hooks",
		"RandomPageQuery": "Hooks",
		"WantedPages::getQueryInfo": "Hooks",
		"ContentHandlerDefaultModelFor": "Hooks",
		"CodeEditorGetPageLanguage": "Hooks"
	},
	"callback": "MediaWiki\\Extension\\KeyPages\\Hooks::onRegistration",
	"manifest_version": 2
}
